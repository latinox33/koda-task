{
    "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "@vue/eslint-config-typescript"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "vue/multi-word-component-names": "off",
        "vue/html-self-closing": "off"
    }
}
